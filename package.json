{
  "name": "spark-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "vite",
    "kill": "fuser -k 5000/tcp",
    "build": "tsc -b --noCheck && vite build",
    "build:pmo": "tsc -b --noCheck && BUILD_TARGET=pmo vite build",
    "build:finanzas": "bash scripts/pre-build-validate.sh && tsc -b --noCheck && BUILD_TARGET=finanzas vite build",
    "lint": "eslint .",
    "test:unit": "VITE_API_BASE_URL=http://localhost tsx --test src/lib/__tests__/*.test.ts",
    "test:behavioral": "tsx --test tests/behavioral-api/*.test.ts",
    "test:ui": "playwright test tests/e2e/behavioral",
    "contract-tests": "BASE=\"${FINZ_API_BASE:-${DEV_API_URL:-}}\"; if [ -z \"$BASE\" ]; then echo \"‚ùå DEV_API_URL or FINZ_API_BASE must be set (e.g. https://.../dev)\"; exit 1; fi; BASE=\"${BASE%/}\"; TOKEN=$(scripts/cognito/get-jwt.sh); npx --yes newman run postman/finanzas-sd-api-collection.json --environment postman/finanzas-sd-dev.postman_environment.json --env-var baseUrl=$BASE --env-var access_token=$TOKEN --env-var username=${COGNITO_TESTER_USERNAME:-$USERNAME} --env-var password=${COGNITO_TESTER_PASSWORD:-$PASSWORD} --env-var cognito_client_id=${COGNITO_WEB_CLIENT} --timeout-request 10000 --timeout-script 5000 --bail",
    "optimize": "vite optimize",
    "preview": "vite preview",
    "preview:pmo": "BUILD_TARGET=pmo vite preview",
    "preview:finanzas": "BUILD_TARGET=finanzas vite preview",
    "test": "node scripts/qa/finanzas-smoke.js",
    "test:e2e:finanzas": "playwright test tests/e2e/finanzas",
    "test:finanzas:changes": "bash tests/finanzas/changes/run-changes-tests.sh",
    "validate:api-config": "bash scripts/validate-api-config.sh",
    "validate:pre-build": "bash scripts/pre-build-validate.sh",
    "smoke:api": "bash scripts/finanzas-e2e-smoke.sh",
    "qa:finanzas:auth": "TS_NODE_PROJECT=tsconfig.node.json node --loader ts-node/esm scripts/qa/verify-finanzas-auth-config.ts",
    "cognito:update-hosted-ui": "bash infra/cognito/update-hosted-ui-css.sh",
    "generate-docs-pdf": "node scripts/generate-docs-pdf.cjs",
    "generate-testing-report-pdf": "bash scripts/generate-testing-report-pdf.sh",
    "render-docs": "node --loader ts-node/esm scripts/docs/render-docs.ts",
    "test:finanzas-ui-diagnostic": "node tests/finanzas-ui-diagnostic/smoke-test.mjs",
    "migrate:handoff-baselines": "node --loader ts-node/esm services/finanzas-api/scripts/migrate-handoff-baseline-projects.ts",
    "diagnose:handoff-baselines": "node --loader ts-node/esm services/finanzas-api/scripts/diagnose-handoff-baseline-collisions.ts"
  },
